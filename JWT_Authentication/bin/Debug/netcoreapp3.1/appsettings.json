{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "AppSettings": {
    "jwtAuth": {
      "jwtSecretKey": "Perpus Secret Key",
      "jwtIssuer": "Perpus Issuer & Audience",
      "jwtExpireMinutes": 1440,
      "jwtIdleExpireMinutes": 30
    },
    "ExternalServices": {
      "host-PerpusApp-service": "http://localhost:5000/PerpusApp"
    },
    "redis": {
      "name": "RedisKey",
      "config": "127.0.0.1:6379"
    }
  }
}
